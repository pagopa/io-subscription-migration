{
  "version": "2.0",
  "logging": {
    "logLevel": {
      "default": "Information"
    },
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "maxTelemetryItemsPerSecond": 5,
        "includedTypes": "PageView;Trace;Dependency;Request",
        "excludedTypes": "Exception;Event;CustomEvent"
      }
    }
  },
  "extensions": {
    "http": {
      "routePrefix": ""
    },
    "queues": {
      "maxPollingInterval": "00:00:15",
      "batchSize": 32
    }
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[2.*, 3.0.0)"
  }
}